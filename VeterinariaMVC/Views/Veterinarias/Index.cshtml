@model IEnumerable<VeterinariaMVC.Models.Veterinaria>

@{
    ViewData["Title"] = "Index";
}

<h1 class="text-center">Veterinarias</h1>


<p>
    <a asp-action="Create" class="btn btn-warning">Agregar Veterinaria</a>

</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ubicacion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Telefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ubicacion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Telefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.VeterinariaId"class="btn btn-success">Editar</a> |
                    <a asp-action="Details" asp-route-id="@item.VeterinariaId" class="btn btn-primary">Detalles</a> |
                    <a asp-action="Delete" asp-route-id="@item.VeterinariaId" class="btn btn-danger">Quitar</a>
            </td>
        </tr>
}
    </tbody>
</table>

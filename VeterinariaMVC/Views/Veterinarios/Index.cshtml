@model IEnumerable<VeterinariaMVC.Models.Veterinario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Agregar Veterinaria</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VeterinariaId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VeterinariaId)
            </td>
            <td>
                    <a asp-action="Edit" asp-route-id="@item.VeterinarioId" class="btn btn-success">Editar</a> |
                    <a asp-action="Details" asp-route-id="@item.VeterinarioId" class="btn btn-primary">Detalles</a> |
                    <a asp-action="Delete" asp-route-id="@item.VeterinarioId" class="btn btn-danger">Eliminar</a>
            </td>
        </tr>
}
    </tbody>
</table>
